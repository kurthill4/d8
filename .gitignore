# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/
/web/sites/*/custom/

#Ignore critical Drupal configuration files
/config/sync/google.analytics.settings.yml
/config/sync/user.*


# Ignore files & custom even if they are symlinks
/web/sites/*/files
/web/sites/*/custom

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore sasscache stuff
.sass-cache*
