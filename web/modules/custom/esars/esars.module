<?php

	/**
	 * Implements hook_theme
	 */
	function esars_theme($existing, $type, $theme, $path) {
		return [
			'esars_block' => [
			'template' => 'block--esars',	// Set Twig template for eSARS block
			],
			'sep_block' => [
			'template' => 'block--sep',	// Set Twig template for SEP block
			],
		];
	}


