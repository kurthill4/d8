{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors":
	[
        {
            "name": "",
            "role": ""
        }
    ],

    "repositories":
	[
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],

    "require": 
	{
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/acl": "^1.0@alpha",
        "drupal/admin_toolbar": "^1.19",
        "drupal/adminimal_theme": "^1.3",
        "drupal/autologout": "^1.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/bootstrap": "^3.4",
        "drupal/bootstrap_layouts": "^4.1",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/calendar": "1.x-dev",
        "drupal/cer": "^4.0@alpha",
        "drupal/coc_forms_auto_export": "^1.0@alpha",
        "drupal/config_filter": "^1.3",
        "drupal/config_ignore": "^2.1",
        "drupal/console": "~1.8",
        "drupal/content_type_clone": "1.x-dev",
        "drupal/convert_bundles": "^1.0@alpha",
        "drupal/core": "~8.6.1",
        "drupal/csv_serialization": "^1.3",
        "drupal/ctools": "^3.0@beta",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_print": "^2.1",
        "drupal/entity_reference_revisions": "^1.2",
        "drupal/entity_reference_views_select": "^1.1",
        "drupal/entity_ui": "^1.2",
        "drupal/feeds": "3.x@dev",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/feeds_ical": "^1.0",
        "drupal/field_group": "^1.0@RC",
        "drupal/field_redirection": "2.x-dev",
        "drupal/field_token_value": "1.x-dev",
        "drupal/file_entity": "^2.0@beta",
        "drupal/google_analytics": "^2.2",
        "drupal/imce": "^1.5",
        "drupal/libraries": "^3.0@dev",
        "drupal/linkit": "^4.3",
        "drupal/markup": "^1.0@beta",
        "drupal/media_entity": "^1.6",
        "drupal/media_entity_document": "^1.1",
        "drupal/media_entity_image": "^1.2",
        "drupal/media_entity_slideshow": "^1.2",
        "drupal/media_entity_twitter": "^1.3",
        "drupal/media_entity_video": "^1.0@beta",
        "drupal/migrate_source_csv": "^2.0",
        "drupal/mobile_detect_twig_extensions": "^1.4",
        "drupal/page_manager": "^1.0@alpha",
        "drupal/paragraphs": "^1.1",
        "drupal/paragraphs_edit": "~2.0@alpha",
        "drupal/paragraphs_previewer": "1.x-dev",
        "drupal/pathauto": "^1.0@RC",
        "drupal/php": "^1.0@beta",
        "drupal/private_taxonomy": "^2.2",
        "drupal/publishcontent": "^1.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/real_aes": "^2.2",
        "drupal/redirect": "^1.0@alpha",
        "drupal/search_api": "^1.11",
        "drupal/search_api_attachments": "^1.0@beta",
        "drupal/search_api_sort_priority": "^1.9",
        "drupal/search_exclude": "^1.1",
        "drupal/slick": "^1.0",
        "drupal/slick_extras": "^1.0@beta",
        "drupal/slick_views": "^1.0@RC",
        "drupal/tablefield": "^2.0@alpha",
        "drupal/telephone_formatter": "^1.0@beta",
        "drupal/telephone_validation": "^2.1",
        "drupal/themable_forms": "^1.0@beta",
        "drupal/twig_blocks": "^2.0",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^2.0",
        "drupal/twitter": "1.x-dev",
        "drupal/twitter_block": "^3.0@alpha",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/video_embed_field": "^1.5",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/views_bootstrap": "3.x-dev",
        "drupal/views_bulk_operations": "^3.2",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_jump_menu": "^1.0@beta",
        "drupal/webform": "^5.0@RC",
        "drupal/webform_encrypt": "^1.0@alpha",
        "drupal/webform_layout_container": "^1.1",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/workbench_access": "^1.0@beta",
        "drupal/youtubechannel": "^3.0",
        "drush/drush": "~9.0",
        "kenwheeler/slick": "^1.6",
        "mikehaertl/phpwkhtmltopdf": "^2.4",
        "oomphinc/composer-installers-extender": "^1.1",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },

    "require-dev": 
	{
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "drupal/config_partial_export": "^1.4",
        "drupal/devel": "^1.2",
        "drupal/migrate_plus": "^4.0@beta",
        "drupal/migrate_tools": "^4.0@beta",
        "drupal/twig_vardumper": "^1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "kint-php/kint": "^2.0",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },

    "conflict": { "drupal/drupal": "*" },
    "minimum-stability": "dev",
    "prefer-stable": true, 
	"config": { "sort-packages": true },
    "autoload": { "classmap": [ "scripts/composer/ScriptHandler.php" ] },
    "scripts": 
	{
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [ "DrupalProject\\composer\\ScriptHandler::checkComposerVersion" ],
        "pre-update-cmd": [ "DrupalProject\\composer\\ScriptHandler::checkComposerVersion" ],
        "post-install-cmd": [ "DrupalProject\\composer\\ScriptHandler::createRequiredFiles" ],
        "post-update-cmd": [ "DrupalProject\\composer\\ScriptHandler::createRequiredFiles" ]
    },

    "extra": 
	{
		"installer-types": ["library"],
        "installer-paths": 
		{
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library", "kenwheeler/slick"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
		
		"patches":
		 {
			"drupal/core": 
			{
				"Role permission to view user email": "https://www.drupal.org/files/issues/2018-12-04/role_based_email_access-drupal-8-6-2799049-17.patch",
				"Fixing debug suggestions for views": "https://www.drupal.org/files/issues/2018-09-11/2118743-179-twig-debug-info.patch",
				"Fixing More link": "https://www.drupal.org/files/issues/2018-06-27/views-more-link-token-replacement_564106_171.patch",
				"First step in seconds issue for Datetime fields": "https://www.drupal.org/files/issues/2723159-43.patch",
				"End date support for Date Range": "https://www.drupal.org/files/issues/2018-05-23/drupal-display-one-date-formatter-2827055-25.patch",
				"Entity reference fix": "https://www.drupal.org/files/issues/2174633-174.patch",
				"Fixing Combined Fields search": "https://www.drupal.org/files/issues/combine_fields_filter-1782678-1_0.patch",
				"Fixing CKEditor multiple styles": "https://www.drupal.org/files/issues/2018-05-18/allow-multiple-elements-2973706-2.patch"
			},

			"drupal/calendar":
			{
				"Patching multiday issue for calendar": "https://www.drupal.org/files/issues/2018-04-06/calendar-date_range-2699477-71.patch",
				"Timezone": "https://www.drupal.org/files/issues/2018-05-09/2604546-33.patch"
			},

			"drupal/entity_reference_views_select": { "Allowing No Selection for entity references": "https://www.drupal.org/files/issues/ervs-emptyvalue-2943488-3-8x.patch" },
			"drupal/feeds_ical":
			{
				"Patching property naming error": "https://www.drupal.org/files/issues/2018-04-25/feeds_ical-item_index-2966206-2_1.patch",
				"Fixing date field issue": "https://www.drupal.org/files/issues/2018-04-25/feeds_ical-dtend-2966214-2.patch",
				"Another field error": "https://www.drupal.org/files/issues/2018-04-25/feeds_ical-date_case-2966211-2.patch"
			},

			"drupal/pathauto": { "Allow to save new path on node after deleting old one": "https://www.drupal.org/files/issues/2018-10-23/pathauto-custom-alias-2756703-17.patch" },
			"drupal/rabbit_hole": { "Double encoding":"https://www.drupal.org/files/issues/2018-11-02/htmlspecialchars_decode-2995993-7.patch" },
			"drupal/views_jump_menu": { "Row index":"https://www.drupal.org/files/issues/views_jump_menu-views_row_index_not_set-2892314-9-D8.patch" }
		
		}
	}
}
